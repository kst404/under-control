{
  "name": "under-control",
  "version": "0.1.2-alpha.1",
  "description": "React Under Control",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/kst404/under-control.git",
  "author": "Kst <kst404@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^5.0.1",
    "jest": "^20.0.3"
  },
  "scripts": {
    "build": "yarn run build:commonjs && yarn run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --ignore __test__ src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel --ignore __test__ src --out-dir es",
    "prepublish": "npm run build",
    "test": "jest --env=node --watch"
  },
  "babel": {
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 4 versions",
                  "not ie < 9"
                ]
              },
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          [
            "transform-object-rest-spread",
            {
              "useBuiltIns": true
            }
          ]
        ]
      },
      "es": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 4 versions",
                  "not ie < 9"
                ]
              },
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-object-rest-spread",
            {
              "useBuiltIns": true
            }
          ]
        ]
      }
    }
  }
}
